#Creating a demo function

def demo_function():
    a = 7 + 8
    return a

#Calling a demo function

a = demo_function()
print(a)

#Creating a function that accepts positioned parameters
def addition(a,b):
    return a + b

print(addition(7,8))

#Creating a function with keyword parameters

def subtraction(a = 20,b = 10):
    return a-b

print(subtraction())
print(subtraction(50, 30))

#Creaing functions with default parameters

def multiplication(a, b =50):
    return a*b

print(multiplication(5))
print(multiplication(20, 30))

#Creating a function that returns multiple types

def personal_info(id, name, age, eligibility):
    a = id
    b = name
    c = age
    d = eligibility
    return a, b, c, d

print(personal_info(101, "Anna", 27, True))


